#: src/liboozie/conf.py:25
msgid "URL of Oozie server. This is required for job submission."
msgstr "URL do Oozie Server. É necessário para a submissão de trabalhos."

#: src/liboozie/conf.py:31
msgid "Whether Oozie requires client to perform Kerberos authentication."
msgstr "Se o Oozie requer que o cliente efectue a autenticação Kerberos ou não."

#: src/liboozie/conf.py:38
msgid "Location on HDFS where the workflows/coordinators are deployed when submitted by a non-owner."
msgstr "Localização no HDFS onde os workflows/coordinators são implementados quando submetidos por um não proprietário."

#: src/liboozie/conf.py:69
msgid "The deployment directory of Oozie workflows does not exist. Run \"Setup App\" on the Oozie workflow page."
msgstr "O directório de implementação dos workflows do Oozie não existe. Execute \"Setup App\" (Configurar Aplicação) na página de workflow do Oozie."

#: src/liboozie/conf.py:72
msgid "Oozie Share Lib not installed in default location."
msgstr "Oozie Share Lib não instalada na localização predefinida."

#: src/liboozie/submittion.py:69
#, python-format
msgid "Submission already submitted (Oozie job id %s)"
msgstr "Submissão já enviada (ID do trabalho do Oozie %s)"

#: src/liboozie/submittion.py:117
msgid "Failed to access deployment directory."
msgstr "Falha ao aceder ao directório de implementação."

#: src/liboozie/submittion.py:162
#, python-format
msgid "Path is not a directory: %s."
msgstr "O caminho não é um directório: %s."

#: src/liboozie/submittion.py:167
#, python-format
msgid "Error accessing directory '%s': %s."
msgstr "Erro ao aceder a directório \"%s\": %s."

#: src/liboozie/types.py:285
msgid "Insufficient permission."
msgstr "Permissão insuficiente."

#: src/liboozie/types.py:286
#, python-format
msgid "Permission denied. User %(username)s cannot modify user %(user)s's job."
msgstr "Permissão recusada. O utilizador %(username)s não pode modificar um trabalho %(user)s do utilizador."

