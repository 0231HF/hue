#: src/liboozie/conf.py:25
msgid "URL of Oozie server. This is required for job submission."
msgstr "URL del servidor Oozie. Es necesaria para el envío de jobs."

#: src/liboozie/conf.py:31
msgid "Whether Oozie requires client to perform Kerberos authentication."
msgstr "Indica si Oozie necesita que el cliente realice la autenticación de Kerberos."

#: src/liboozie/conf.py:38
msgid "Location on HDFS where the workflows/coordinators are deployed when submitted by a non-owner."
msgstr "Ubicación, en HDFS, en la que se implementan los workflows/coordinators cuando no los envía el propietario."

#: src/liboozie/conf.py:59
msgid "The deployment directory of Oozie workflows does not exist. Run \"Setup Examples\" on the Oozie workflow page."
msgstr "El directorio de implementación de los workflows de Oozie no existe. Ejecute \"Configurar ejemplos\" en la página del workflow de Oozie."

#: src/liboozie/conf.py:62
msgid "Oozie Share Lib not installed in default location."
msgstr "Share Lib de Oozie no instalada en la ubicación predeterminada."

#: src/liboozie/submittion.py:68
#, python-format
msgid "Submission already submitted (Oozie job id %s)"
msgstr "Envío ya enviado (ID de trabajo de Oozie %s)"

#: src/liboozie/submittion.py:147
msgid "Failed to access deployment directory."
msgstr "Error al acceder al directorio de implementación."

#: src/liboozie/submittion.py:204
#, python-format
msgid "Path is not a directory: %s."
msgstr "La ruta no es un directorio: %s."

#: src/liboozie/submittion.py:209
#, python-format
msgid "Error accessing directory '%s': %s."
msgstr "Error al acceder al directorio '%s': %s."

#: src/liboozie/types.py:356
msgid "Insufficient permission."
msgstr "Permiso insuficiente."

#: src/liboozie/types.py:357
#, python-format
msgid "Permission denied. User %(username)s cannot modify user %(user)s's job."
msgstr "Permiso denegado. El usuario %(username)s no puede modificar el job del usuario %(user)s."

